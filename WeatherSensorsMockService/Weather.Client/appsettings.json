{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceConfig": {
    "AggregationIntervalTime": 1,
    "ReconnectionAmount": 3,
    "ReconnectionTimeout": 15
  },
  "ExternalApis": {
    "WeatherEmulator": {
      "Url": "https://localhost:7236/",
      "SendMessageSize": 1024,
      "ReceiveMessageSize": 1024
    }
  }
}
